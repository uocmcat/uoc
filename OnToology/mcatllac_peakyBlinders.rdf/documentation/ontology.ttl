@prefix : <http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#> .

<http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders> rdf:type owl:Ontology ;
                                                                   rdfs:comment "Peaky Blinders ontology. Autor: Marc Catll√†." .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:domain :Sites ;
             rdfs:range :Cities ;
             rdfs:comment "Sites are located in one city." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#inSeason
:inSeason rdf:type owl:ObjectProperty ;
          rdfs:domain :Characters ;
          rdfs:range :Seasons ;
          rdfs:comment "Season in which character appeared." .


###  https://dbpedia.org/page/Birmingham#availablePlatf
<https://dbpedia.org/page/Birmingham#availablePlatf> rdf:type owl:ObjectProperty ;
                                                     rdfs:domain :Seasons ;
                                                     rdfs:range :TVPlatforms ;
                                                     rdfs:comment "Platform where sere is available" .


###  https://dbpedia.org/page/Birmingham#category
<https://dbpedia.org/page/Birmingham#category> rdf:type owl:ObjectProperty ;
                                               rdfs:domain :Nominated ;
                                               rdfs:range :Category ;
                                               rdfs:comment "Category of the nomination award." .


###  https://dbpedia.org/page/Birmingham#categoryW
<https://dbpedia.org/page/Birmingham#categoryW> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <https://dbpedia.org/page/Birmingham#category> ;
                                                rdfs:domain :Won ;
                                                rdfs:range :Category ;
                                                rdfs:comment "Category of the won award." .


###  https://dbpedia.org/page/Birmingham#createdBy
<https://dbpedia.org/page/Birmingham#createdBy> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf owl:topObjectProperty ;
                                                rdfs:domain <https://dbpedia.org/page/Birmingham#peaky_blinders> ;
                                                rdfs:range :Creator ;
                                                rdfs:comment "Creator of the serie." .


###  https://dbpedia.org/page/Birmingham#hasDirector
<https://dbpedia.org/page/Birmingham#hasDirector> rdf:type owl:ObjectProperty ;
                                                  rdfs:domain :Seasons ;
                                                  rdfs:range :Director ;
                                                  rdfs:comment "Every Season has at least one Director." .


###  https://dbpedia.org/page/Birmingham#inYear
<https://dbpedia.org/page/Birmingham#inYear> rdf:type owl:ObjectProperty ;
                                             rdfs:domain :Seasons ;
                                             rdfs:range :Year ;
                                             rdfs:comment "Year that season is distributed." .


###  https://dbpedia.org/page/Birmingham#interpretedBy
<https://dbpedia.org/page/Birmingham#interpretedBy> rdf:type owl:ObjectProperty ;
                                                    rdfs:domain :Characters ;
                                                    rdfs:range <https://dbpedia.org/page/Birmingham#Cast> ;
                                                    rdfs:comment "Actor or actress interpreted." .


###  https://dbpedia.org/page/Birmingham#lang
<https://dbpedia.org/page/Birmingham#lang> rdf:type owl:ObjectProperty ;
                                           rdfs:domain :Seasons ;
                                           rdfs:range :Original ;
                                           rdfs:comment "Original language." .


###  https://dbpedia.org/page/Birmingham#longVOS
<https://dbpedia.org/page/Birmingham#longVOS> rdf:type owl:ObjectProperty ;
                                              rdfs:subPropertyOf <https://dbpedia.org/page/Birmingham#lang> ;
                                              rdfs:domain :Seasons ;
                                              rdfs:range :Doubled ;
                                              rdfs:comment "Doubled languages for season." .


###  https://dbpedia.org/page/Birmingham#needCostume
<https://dbpedia.org/page/Birmingham#needCostume> rdf:type owl:ObjectProperty ;
                                                  rdfs:domain :Characters ;
                                                  rdfs:range :Costume ;
                                                  rdfs:comment "Actors need differetn costumes." .


###  https://dbpedia.org/page/Birmingham#seasonSound
<https://dbpedia.org/page/Birmingham#seasonSound> rdf:type owl:ObjectProperty ;
                                                  rdfs:domain :Soundtrack ;
                                                  rdfs:range :Seasons ;
                                                  rdfs:comment "Seasons with this songs." .


#################################################################
#    Data properties
#################################################################

###  https://dbpedia.org/page/Birmingham#genere
<https://dbpedia.org/page/Birmingham#genere> rdf:type owl:DatatypeProperty ;
                                             rdfs:domain :Genere ;
                                             rdfs:range [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:string ;
                                                          owl:withRestrictions ( [ xsd:minInclusive "3"
                                                                                 ]
                                                                                 [ xsd:maxInclusive "12"
                                                                                 ]
                                                                               )
                                                        ] .


###  https://dbpedia.org/page/Birmingham#year
<https://dbpedia.org/page/Birmingham#year> rdf:type owl:DatatypeProperty ;
                                           rdfs:domain :Year ;
                                           rdfs:range [ rdf:type rdfs:Datatype ;
                                                        owl:onDatatype xsd:integer ;
                                                        owl:withRestrictions ( [ xsd:minInclusive 1950
                                                                               ]
                                                                               [ xsd:maxInclusive 2021
                                                                               ]
                                                                             )
                                                      ] .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Actor
:Actor rdf:type owl:Class ;
       rdfs:subClassOf <https://dbpedia.org/page/Birmingham#Cast> ;
       owl:disjointWith <https://dbpedia.org/page/Birmingham#Actress> ;
       rdfs:comment "Actors of the serie." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Awards
:Awards rdf:type owl:Class ;
        rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ;
        rdfs:comment "All won and nominated awards." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Category
:Category rdf:type owl:Class ;
          rdfs:subClassOf :Awards ;
          rdfs:comment "Category of the award." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Characters
:Characters rdf:type owl:Class ;
            rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :inSeason ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :Seasons
                            ] ;
            rdfs:comment "Cast of the serie." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Cities
:Cities rdf:type owl:Class ;
        rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ;
        rdfs:comment "Filming cities" .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Costume
:Costume rdf:type owl:Class ;
         rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ;
         rdfs:comment "Differents costumes and dresses used during the filming." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Creator
:Creator rdf:type owl:Class ;
         rdfs:subClassOf :Person ;
         rdfs:comment "Creator of the serie." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Director
:Director rdf:type owl:Class ;
          rdfs:subClassOf :Person ;
          rdfs:comment "Directors of the serie." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Distributor
:Distributor rdf:type owl:Class ;
             rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ;
             rdfs:comment "Companies that dstribute the serie around the world." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Doubled
:Doubled rdf:type owl:Class ;
         rdfs:subClassOf :Language ;
         rdfs:comment "Languages doubled." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Female
:Female rdf:type owl:Class ;
        rdfs:subClassOf :Person ;
        owl:disjointWith :Male ;
        rdfs:comment "Females related with the serie." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Genere
:Genere rdf:type owl:Class ;
        rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ;
        rdfs:comment "Most common generes in episodes." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Guest
:Guest rdf:type owl:Class ;
       rdfs:subClassOf :Characters ;
       owl:disjointWith :Main ,
                        :Secondary ;
       rdfs:comment "Characters that appears one chapter." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Language
:Language rdf:type owl:Class ;
          rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ;
          rdfs:comment "Languages availables." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Main
:Main rdf:type owl:Class ;
      rdfs:subClassOf :Characters ;
      owl:disjointWith :Secondary ;
      rdfs:comment "Main characters." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Male
:Male rdf:type owl:Class ;
      rdfs:subClassOf :Person ;
      rdfs:comment "Males related with the serie." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Nominated
:Nominated rdf:type owl:Class ;
           rdfs:subClassOf :Awards ;
           rdfs:comment "Award nomination." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Original
:Original rdf:type owl:Class ;
          rdfs:subClassOf :Language ;
          rdfs:comment "Original Language." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ;
        rdfs:comment "People who participates." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Producer
:Producer rdf:type owl:Class ;
          rdfs:subClassOf :Person ;
          rdfs:comment "Producer of the serie." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Seasons
:Seasons rdf:type owl:Class ;
         rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <https://dbpedia.org/page/Birmingham#hasDirector> ;
                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Director
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <https://dbpedia.org/page/Birmingham#inYear> ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Year
                         ] ;
         rdfs:comment "Serie seasons." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Secondary
:Secondary rdf:type owl:Class ;
           rdfs:subClassOf :Characters ;
           rdfs:comment "Secondary characters." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Sites
:Sites rdf:type owl:Class ;
       rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasLocation ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Cities
                       ] ;
       rdfs:comment "Main filming sites." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Soundtrack
:Soundtrack rdf:type owl:Class ;
            rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <https://dbpedia.org/page/Birmingham#seasonSound> ;
                              owl:someValuesFrom :Seasons
                            ] ;
            rdfs:comment "All serie songs." ;
            rdfs:sameAs "https://dbpedia.org/page/Soundtrack" .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#TVPlatforms
:TVPlatforms rdf:type owl:Class ;
             rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ;
             rdfs:comment "Platforms where serie is available." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Won
:Won rdf:type owl:Class ;
     rdfs:subClassOf :Awards ;
     rdfs:comment "Award won." .


###  http://www.semanticweb.org/marc/ontologies/2022/3/peakyblinders#Year
:Year rdf:type owl:Class ;
      rdfs:subClassOf <https://dbpedia.org/page/Birmingham#peaky_blinders> ;
      rdfs:comment "Years of seasons and Awards" .


###  https://dbpedia.org/page/Birmingham#Actress
<https://dbpedia.org/page/Birmingham#Actress> rdf:type owl:Class ;
                                              rdfs:subClassOf <https://dbpedia.org/page/Birmingham#Cast> ;
                                              rdfs:comment "Actresses of the serie" .


###  https://dbpedia.org/page/Birmingham#Cast
<https://dbpedia.org/page/Birmingham#Cast> rdf:type owl:Class ;
                                           rdfs:subClassOf :Person ;
                                           rdfs:comment "Cast of the serie." .


###  https://dbpedia.org/page/Birmingham#peaky_blinders
<https://dbpedia.org/page/Birmingham#peaky_blinders> rdf:type owl:Class ;
                                                     rdfs:comment "Peaky Blinders Class" .


